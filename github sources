angular application
    npm install -g @angular/cli
    ng version
    ng new applicationName
    for zoneless - no
    select css
    cd applicationName
    ng serve
    ng g c componentName
    ng g s serviceName
    cntrl c to exit
    directives ->  ng g d
    pipes -> ng g pipes
    routes


mongodb
    use dbName
    db.createCollection('collectionName')
    db.collectionName.insertOne({_,_})
    .exit

